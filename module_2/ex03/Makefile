NAME=eval_expr
FLAGS=-Wall -Wextra -Werror
CC=clang++
FILES=Fixed.cpp main.cpp utils.cpp parse.cpp

$(NAME): Fixed.hpp header.h
	#$(CC) -g $(FLAGS) $(FILES) -o $(NAME)
	$(CC) -g $(FILES) -o $(NAME)

all: $(NAME)

clean:
	rm -f $(NAME)
	rm -rf $(NAME).dSYM

re: clean all

run: $(NAME)
	./$(NAME)